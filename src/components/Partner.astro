---
import { Image } from "astro:assets";

interface Props {
    src: ImageMetadata;
    alt: string;
    url: string;
}
const { src, alt, url } = Astro.props as Props;
---

<div class="h-14 md:h-24 mx-10 my-4">
    <a href={url} target="_blank">
        <Image
            src={src}
            alt={alt}
            inferSize
            quality="low"
            decoding="async"
            loading="lazy"
            class="h-full w-auto"
        />
    </a>
</div>
